{
  "name": "beelup-lite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cleanup": "tsx scripts/cleanup-expired-holds.ts",
    "worker": "node --env-file=.env.local worker/index.mjs",
    "job:post": "ts-node app/api/jobs/[id]/route.ts",
    "job:validate": "ts-node -e \"require('./app/api/jobs/[id]/route.ts')\"",
    "dev:all": "concurrently -n WEB,WORKER -c blue,magenta \"npm run dev\" \"npm run worker\"",
    "dev:web": "npm run dev",
    "dev:worker": "npm run worker"
  },
