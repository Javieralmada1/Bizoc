    "@tsparticles/engine": "^3.9.1",
    "@types/pg": "^8.15.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "execa": "^9.6.0",
    "fluent-ffmpeg": "^2.1.3",
    "lucide-react": "0.471.0",
    "nanoid": "^5.1.5",
